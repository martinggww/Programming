package DesignPattern.OOPBasic;

public class Griffe extends Creature{

	@Override
	public void setName(String newName) {
		// TODO Auto-generated method stub
		name = newName;
	}

	@Override
	public String getName() {
		// TODO Auto-generated method stub
		return name;
	}

	@Override
	public void setWeight(int newWeight) {
		// TODO Auto-generated method stub
		weight = newWeight;
	}

}
